{
  "name": "react-bound",
  "version": "0.1.3",
  "description":
    "a HOC for data-binding an input to a model-it can be a POJO or a mobx observable",
  "main": "dist/react-bound.cjs.js",
  "module": "dist/react-bound.es.js",
  "scripts": {
    "test": "BABEL_ENV=test ava",
    "pretest": "node_modules/.bin/standard --fix",
    "precommit": "npm test",
    "showcase": "cd e2e-test && npx webpack-dev-server",
    "build": "BABEL_ENV=rollup rollup -c",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/capaj/react-bound.git"
  },
  "keywords": ["react", "data", "inputs", "binding"],
  "author": "capajj@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/capaj/react-bound/issues"
  },
  "files": ["dist"],
  "homepage": "https://github.com/capaj/react-bound#readme",
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "react": "^16.0.0-rc.3",
    "react-dom": "^16.0.0-rc.3",
    "react-json-view": "^1.12.4",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "standard": "^10.0.3",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "dependencies": {
    "dateformat": "^3.0.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.0",
    "lodash.set": "^4.3.2",
    "mobx": "^3.3.0",
    "mobx-react": "^4.3.2",
    "prop-types": "^15.5.10"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": ["dist/", "flow-typed/", "src/common/"],
    "env": "browser"
  },
  "ava": {
    "files": ["**/*.spec.js"],
    "babel": "inherit",
    "require": ["babel-register"]
  }
}
