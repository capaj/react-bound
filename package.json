{
  "name": "react-bound",
  "version": "0.4.1",
  "description": "a HOC for data-binding an input to a model-it can be a POJO or a mobx observable",
  "main": "dist/react-bound.cjs.js",
  "module": "dist/react-bound.es.js",
  "scripts": {
    "test": "BABEL_ENV=test ava",
    "pretest": "node_modules/.bin/standard --fix",
    "precommit": "npm test",
    "showcase": "cd e2e-test && npx webpack-dev-server",
    "build": "BABEL_ENV=rollup rollup -c",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/capaj/react-bound.git"
  },
  "keywords": [
    "react",
    "data",
    "inputs",
    "binding"
  ],
  "author": "capajj@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/capaj/react-bound/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/capaj/react-bound#readme",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "mobx": ">=4.3.0",
    "mobx-react": "^5.1.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-json-view": "^1.17.0",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^3.0.4",
    "standard": "^11.0.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  },
  "peerDependencies": {
    "react": ">=16",
    "mobx": ">=3",
    "mobx-react": ">=4"
  },
  "dependencies": {
    "dateformat": "^3.0.3",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.1",
    "lodash.set": "^4.3.2",
    "prop-types": "^15.6.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist/",
      "flow-typed/",
      "src/common/"
    ],
    "env": "browser"
  },
  "ava": {
    "files": [
      "**/*.spec.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}
