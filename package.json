{
  "name": "react-bound",
  "version": "0.0.8",
  "description": "a HOC for data-binding an input to a model-it can be a POJO or a mobx observable",
  "main": "dist/react-bound.cjs.js",
  "module": "dist/react-bound.es.js",
  "scripts": {
    "test": "BABEL_ENV=test ava",
    "pretest": "node_modules/.bin/standard",
    "precommit": "npm test",
    "showcase": "cd e2e-test && npx webpack-dev-server",
    "build": "BABEL_ENV=rollup rollup -c",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/capaj/react-bound.git"
  },
  "keywords": [
    "react",
    "data",
    "inputs",
    "binding"
  ],
  "author": "capajj@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/capaj/react-bound/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/capaj/react-bound#readme",
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "react": "^16.0.0-beta.5",
    "react-dom": "^16.0.0-beta.5",
    "react-json-view": "^1.12.0",
    "rollup": "^0.48.2",
    "rollup-plugin-babel": "^3.0.2",
    "standard": "^10.0.3",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "dependencies": {
    "dateformat": "^2.0.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "mobx": "^3.2.2",
    "mobx-react": "^4.2.2",
    "prop-types": "^15.5.10"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist/",
      "flow-typed/",
      "src/common/"
    ],
    "env": "browser"
  },
  "ava": {
    "files": [
      "**/*.spec.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}
